{
  "short_name": "UT",
  "name": "Udemy Timer",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "index.html",
    "default_title": "Udemy Timer"
  },
  "permissions": ["tabs", "activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.udemy.com/course/*"],
      "js": ["./static/js/content.js"]
    }
  ]
}
